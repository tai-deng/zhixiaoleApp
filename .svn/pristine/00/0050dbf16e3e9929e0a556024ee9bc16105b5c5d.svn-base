.mui-table-view.mui-grid-view .mui-table-view-cell .mui-media-body
{
    font-size: 14px;
    line-height: 15px;
    display: block;
    width: 100%;
    height: auto;
    margin-top: 8px;
    text-overflow: ellipsis;
    color: #333;
}
.padding-0-10px{
    padding: 0px 10px !important;
}
.width80{
    width: 80% !important;
}
.width70{
   width: 70% !important;
}
#up_zzjs{
    width:100%;height:22px;line-height:20px;overflow:hidden;
}
#up_zzjs #up_li{list-style-type:none;margin:0;padding:0;}
#up_zzjs #up_li a{
    font-size:14px;line-height: 16px;
    margin-bottom: 5px;
}
.noticeText{
    width: 20%; border-right: 1px solid #eeeeee;padding-left: 5px;
}
.noticeLittleImg{
    width: 5%;padding-left: 5px;
}
.noticeUp{
    width: 70% !important;padding-left: 10px;overflow: hidden;
}
.noticeContent{
    color:#000000;line-height: 20px;white-space: nowrap;overflow: hidden;
    text-overflow: ellipsis !important; -o-text-overflow:ellipsis;-moz-text-overflow:ellipsis;
}
.mui-table-view-cell:after {
    position: absolute;
    right: 0;
    bottom: 0;
    left: 0px;
    height: 1px;
    content: '';
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    /* background-color: #eeeeee; */
    background-color: #eeeeee;
}
