    <style>
        .mui-popup-buttons {
            height: 60px;
        }
        .mui-popup-button {
            line-height: 60px;
            height: 60px;
        }
        .mui-input-row select {
            /* font-size: 17px; */
            height: 37px;
            padding: 0 10px !important;
            background: rgba(255, 255, 255, .95) !important;
        }
        .mui-popup-inner:after {
            background-color: #fff !important;
        }
        .mui-popup-inner {
            background-color: #fff !important;
        }
        .mui-popup-button {
            background-color: #fff !important;
        }
        .mui-input-row label {
            padding: 11px 10px;
        }
        .mui-input-row label ~ input, .mui-input-row label ~ select, .mui-input-row label ~ textarea {
            width: 70%;
        }
        .mui-input-row label {
            width: 30%;
        }
        @media only screen and (min-device-width: 414px) and (max-device-width: 736px) and (orientation : portrait) {
             /* iphone6 plus竖屏*/
            .mui-popup {
                width: 350px;
            }
        }
        .down-pull {
            position: absolute;
            top: 0;
            line-height: 37px;
            right: 10px;
        }
        .margin-top10 {
            margin-top: 10px;
        }
        .title {
            position: absolute;
            top: 15px;
            left: 43%;
        }

        .chongzhifuwu-cont {
            position: absolute;
            top: 40%;
            left: 10%;
            width: 80%;
            overflow: hidden;
        }
        .chongzhifuwu-item {
            border-bottom: 1px solid #eee;
            color: #888;
        }
        .chongzhijf {
            top: 0;
            left: 0;
            position: fixed;
            z-index: 100;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, .3);
        }
        .chongzhifuwu-item {
            clear: left;
            overflow: hidden;
            position: relative;
        }
        .chongzhifuwu-item select,.chongzhifuwu-item input{
            margin-bottom: 0;
        }
        #qitataocanxufei-deatil .tip{
            padding-left: 10px;
        }

        .butns:after {
            content: "";
            display: block;
            height: 0;
            clear: both;
            visibility: hidden;
        }
        .butns {
            zoom: 1;
        }
        .butns .butn{
            width: 35%;
            height: 30px;
            line-height: 30px;
            border-radius: 20px;
        }
    </style>
</head>
<body>
<!--<header class="mui-bar mui-bar-nav horbackground">
