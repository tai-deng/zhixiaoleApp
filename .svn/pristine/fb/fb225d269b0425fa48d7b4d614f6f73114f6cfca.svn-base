/*ËùÓÐ*/
@media screen and (min-width: 320px) and (max-width: 480px) {
    .yuan{
        width:90px;
        height:90px;
        border-radius:90px 0 0 0;
    }
    .yuan2{
        width:90px;
        height:90px;
        border-radius: 0 90px 0 0;
    }
    .yuan3{
        width:90px;
        height:90px;
        border-radius: 0 0 90px 0;
    }
    .yuan4{
        width:90px;
        height:90px;
        border-radius: 0 0 0 90px;
    }
    .container{
        position: absolute; top: 50px;left: 25%;width: 183px
    }
    .centerYuan{
        width: 90px;height: 90px;border-radius: 45px; position: absolute;
        left: 46px;top:46px;
    }
    .lineHeight{
        line-height: 90px;
    }
    .shuText{
        width: 30px;padding-left: 40%;padding-top: 30%;
    }

}
