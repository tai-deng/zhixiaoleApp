    <div class="mui-input-row line60 gray-8color grayborder1 ridus30px" style="margin: 10px 15px 0px 15px;">
            <label>
                <span class="mui-icon iconfont icon-mima54 font26"></span>
            </label>
            <input id='password' type="password" onpaste="return false" ondragenter="return false" onkeyup="value=value.replace(/[\W]/g,'')" class="mui-input-clear mui-input font14 line60 new-input" placeholder="设置登录密码" >
    </div>
    <div class="mui-input-row line60 gray-8color grayborder1 ridus30px" style="margin: 10px 15px 0px 15px;">
