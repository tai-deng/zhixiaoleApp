.z_photo img {
    width: 1rem;
    height: 1rem;
    border-radius: 5px;

}
.z_photo{
    color: #dddddd;
}

.z_addImg {
    float: left;
    margin-right: 0.2rem;
}

.z_file {
    width: 1rem;
    height: 1rem;
    background: url('../img/img.png') no-repeat;
    background-size: 100% 100%;
    float: left;
    margin-right: 0.2rem;
}

.z_file input::-webkit-file-upload-button {
    width: 1rem;
    height: 1rem;
    border: none;
    position: absolute;
    outline: 0;
    opacity: 0;
}

.z_file input#file {
    display: block;
    width: auto;
    border: 0;
    vertical-align: middle;
}
